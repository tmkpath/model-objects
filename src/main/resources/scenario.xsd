<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" version="2.0.2">
<xs:include schemaLocation="network.xsd"/>

<!-- common ......................................................... -->

   <!-- used by event, controller -->
   <xs:element name="targetElements">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="scenarioElement" minOccurs="0" 
               maxOccurs="unbounded" />
         </xs:sequence>
      </xs:complexType>
   </xs:element>

<!-- settings ....................................................... -->

   <xs:element name="settings">
      <xs:complexType>
         <xs:all>
            <xs:element ref="units" minOccurs="1" />
         </xs:all>
      </xs:complexType>
   </xs:element>

   <!--
         SI ...     length in meters, time in seconds
         US ...     length in miles,  time in hours
         Metric ... length in km,     time in hours
   -->
   <xs:element name="units">
      <xs:simpleType>
         <xs:restriction base="xs:string">
            <xs:pattern value="US|Metric|SI" />
         </xs:restriction>
      </xs:simpleType>
   </xs:element>

<!-- VehicleTypeSet .................................................. -->

   <xs:element name="VehicleTypeSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="vehicleType" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="project_id" type="xs:long" use="optional" 
            default="0" />
      </xs:complexType>
   </xs:element>

   <xs:element name="vehicleType">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="required" />
         <xs:attribute name="size_factor" type="xs:double" use="optional" 
            default="1" />
         <xs:attribute name="is_standard" type="xs:int" use="optional" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
      </xs:complexType>
   </xs:element>

<!-- scenario ........................................................ -->

   <xs:element name="scenario">
      <xs:complexType>
         <xs:all>
            <xs:element ref="description" minOccurs="0" maxOccurs="1" />
            <xs:element ref="settings" minOccurs="0"  maxOccurs="1" />
            <xs:element ref="NetworkSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="VehicleTypeSet" minOccurs="1" />
            <xs:element ref="SensorSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="ActuatorSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="InitialDensitySet" minOccurs="0" 
               maxOccurs="1" />
            <xs:element ref="WeavingFactorSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="SplitRatioSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="DownstreamBoundaryCapacitySet" minOccurs="0" 
               maxOccurs="1" />
            <xs:element ref="EventSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="DemandSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="ControllerSet" minOccurs="0" maxOccurs="1" />
            <xs:element ref="FundamentalDiagramSet" minOccurs="0" 
               maxOccurs="1" />
            <xs:element ref="NetworkConnections" minOccurs="0" 
               maxOccurs="1" />
            <xs:element ref="DestinationNetworks" minOccurs="0" 
               maxOccurs="1" />
            <xs:element ref="RouteSet" minOccurs="0" />
         </xs:all>
         <xs:attribute name="project_id" type="xs:long" use="optional" 
            default="0" />
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="schemaVersion" type="xs:string" 
            use="required" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
         <xs:attribute name="lockedForEdit" type="xs:boolean" use="optional" 
            default="false" />
         <xs:attribute name="lockedForHistory" type="xs:boolean" 
            use="optional" default="false" />
      </xs:complexType>
   </xs:element>

<!-- Scenario Set Type -->

   <xs:element name="ScenarioSetType">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="required"/>
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="in_use" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
      </xs:complexType>
   </xs:element>

<!-- NetworkSet ..................................................... -->

   <xs:element name="NetworkSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="network" minOccurs="0" maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="project_id" type="xs:long" use="required" />
      </xs:complexType>
   </xs:element>

<!-- SignalPlanSet .................................................... -->

   <xs:element name="SignalPlanSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalPlan" minOccurs="1" 
                maxOccurs="unbounded" />
            <xs:element ref="SignalPlanScheduleSet" minOccurs="0" 
                maxOccurs="1" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="project_id" type="xs:long" use="required"/>
         <xs:attribute name="controller_id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="default_plan_id" type="xs:long" 
            use="required" />
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPlan">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalPhase" minOccurs="1" 
               maxOccurs="unbounded" />
            <xs:element ref="SignalTimingSet" minOccurs="1" maxOccurs="1" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="plan_set_id" type="xs:long" use="required"/>
         <xs:attribute name="offset_ms" type="xs:long" use="required"/>
         <xs:attribute name="cycle_length_ms" type="xs:long" use="required"/>
         <xs:attribute name="start_time" type="xs:long" use="optional"/>
         <xs:attribute name="stop_time" type="xs:long" use="optional"/>
         <xs:attribute name="reference_point" type="xs:long" use="optional"/>
         <xs:attribute name="reference_phase" type="xs:long" use="optional"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPlanScheduleSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalPlanSchedule" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="plan_set_id" type="xs:long" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPlanSchedule">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalPlanScheduleElement" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="schedule_set_id" type="xs:long" use="required"/>
         <xs:attribute name="table_num" type="xs:long" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPlanScheduleElement">
      <xs:complexType>
         <xs:sequence>
            <xs:element name="start_time" type="dateTime" minOccurs="1" 
               maxOccurs="1" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="schedule_id" type="xs:long" use="required"/>
         <xs:attribute name="action_type" type="xs:long" use="required"/>
         <xs:attribute name="action_item" type="xs:string" use="required"/>
         <xs:attribute name="Saturday" type="xs:boolean" use="required"/>
         <xs:attribute name="Sunday" type="xs:boolean" use="required"/>
         <xs:attribute name="Monday" type="xs:boolean" use="required"/>
         <xs:attribute name="Tuesday" type="xs:boolean" use="required"/>
         <xs:attribute name="Wednesday" type="xs:boolean" use="required"/>
         <xs:attribute name="Thursday" type="xs:boolean" use="required"/>
         <xs:attribute name="Friday" type="xs:boolean" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPhase">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalPhaseMovement" minOccurs="0" 
               maxOccurs="1" />
            <xs:element ref="SignalPhaseElement" minOccurs="0" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="plan_id" type="xs:long" use="required"/>
         <xs:attribute name="phase_number" type="xs:int" use="required"/>
         <xs:attribute name="in_use" type="xs:int" use="required" />
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPhaseElement">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="type" type="xs:long" use="required" />
         <xs:attribute name="phase_length_ms" type="xs:long" 
            use="required" />
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPhaseMovement">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalMovementElement" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="plan_id" type="xs:long" use="required"/>
         <xs:attribute name="phase_number" type="xs:int" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalMovementElement">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="movement_num" type="xs:int" use="required"/>
         <xs:attribute name="link_id_to" type="xs:long" use="required"/>
         <xs:attribute name="link_id_from" type="xs:long" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalTimingSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalTiming" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="plan_id" type="xs:long" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalTiming">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalTimingEntry" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="timing_set_id" type="xs:long" use="required"/>
         <xs:attribute name="type" type="xs:long" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalTimingEntry">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="SignalTimingElement" minOccurs="1" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="order" type="xs:int" use="required"/>
         <xs:attribute name="time_step_ms" type="xs:long" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalTimingElement">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="phase_num" type="xs:int" use="required"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalPhaseElementType">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="required" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="in_use" type="xs:int" use="optional" />
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalTimingType">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="required" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="in_use" type="xs:int" use="optional" />
      </xs:complexType>
   </xs:element>

   <xs:element name="SignalScheduleActionType">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="name" type="xs:string" use="required" />
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional"/>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="in_use" type="xs:int" use="optional" />
      </xs:complexType>
   </xs:element>

<!-- SensorSet ........................................................ -->

   <xs:element name="SensorSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="sensor" minOccurs="0" maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="project_id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="optional" 
            default="" />
         <xs:attribute name="description" type="xs:string" use="optional" 
            default="0" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
         <xs:attribute name="lockedForEdit" type="xs:boolean" use="optional" 
            default="false" />
         <xs:attribute name="lockedForHistory" type="xs:boolean" 
            use="optional" default="false" />
      </xs:complexType>
   </xs:element>

   <xs:element name="sensor">
      <xs:complexType>
         <xs:all>
            <xs:element ref="display_position" minOccurs="0" />
            <xs:element ref="parameters" minOccurs="0" />
            <xs:element ref="table" minOccurs="0" />
            <xs:element ref="sensor_type" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="usageType" minOccurs="0" maxOccurs="1"/>
         </xs:all>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="network_id" type="xs:long" use="optional" />
         <xs:attribute name="link_id" type="xs:long" use="optional" />
         <xs:attribute name="link_type_original" type="xs:string" 
            use="optional" />
         <xs:attribute name="java_class" type="xs:string" use="optional" />
         <xs:attribute name="sensor_id_original" type="xs:string" 
            use="optional" />
         <xs:attribute name="data_feed_id" type="xs:long" use="optional" />
         <xs:attribute name="link_offset" type="xs:double" use="optional" />
         <xs:attribute name="lane_number" type="xs:int" use="optional" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" />
         <xs:attribute name="health_status" use="optional">
            <xs:simpleType >
               <xs:restriction base="xs:double">
                  <xs:minInclusive value="0.0"/>
                  <xs:maxInclusive value="1.0"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:attribute>
      </xs:complexType>
   </xs:element>

<!-- Sensor Type -->
   <xs:element name="sensor_type">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="required"/>
         <xs:attribute name="description" type="xs:string" use="optional"/>
         <xs:attribute name="in_use" type="xs:string" use="optional"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
      </xs:complexType>
   </xs:element>

<!-- Master Actuator Set -->
   <xs:element name="ActuatorSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="description" minOccurs="0" />
            <xs:element ref="actuator" minOccurs="0" maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="project_id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="optional" 
            default="" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
         <xs:attribute name="lockedForEdit" type="xs:boolean" use="optional" 
            default="false" />
         <xs:attribute name="lockedForHistory" type="xs:boolean" 
            use="optional" default="false" />
      </xs:complexType>
   </xs:element>

<!-- Actuator - Needed for Controller -->
   <xs:element name="actuator">
      <xs:complexType>
         <xs:all>
            <xs:element ref="scenarioElement" minOccurs="0" maxOccurs="1" />
            <xs:element ref="actuator_type" minOccurs="0" maxOccurs="1" />
            <xs:element ref="display_position" minOccurs="0" maxOccurs="1"/>
            <xs:element ref="parameters" minOccurs="0" />
            <xs:element ref="table" minOccurs="0" />
            <xs:element ref="usageType" minOccurs="0" maxOccurs="1"/>
         </xs:all>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="optional" />
         <xs:attribute name="network_id" type="xs:long" use="optional" />
         <xs:attribute name="actuator_id_original" type="xs:string" 
            use="optional" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
      </xs:complexType>
   </xs:element>

   <xs:element name="actuator_type">
      <xs:complexType>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="required" />
         <xs:attribute name="description" type="xs:string" use="optional" />
         <xs:attribute name="in_use" type="xs:int" use="optional" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
      </xs:complexType>
   </xs:element>

<!-- InitialDensitySet ............................................... -->

   <xs:element name="InitialDensitySet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="description" minOccurs="0" />
            <xs:element ref="density" minOccurs="0" maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="project_id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="optional" 
            default="" />
         <!-- time stamp in [units of time] after midnight -->
         <xs:attribute name="tstamp" type="xs:double" use="optional" 
            default="0.0"/>
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" />
      </xs:complexType>
   </xs:element>

   <xs:element name="density">
      <xs:complexType mixed="true"><!-- text in vehicles/[unit of length] -->
         <xs:attribute name="vehicle_type_id" type="xs:long" 
            use="required" />
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="link_id" type="xs:long" use="required" />
         <xs:attribute name="destination_network_id" type="xs:long" 
            use="optional" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" />
      </xs:complexType>
   </xs:element>

<!-- WeavingFactorSet ................................................ -->

   <xs:element name="WeavingFactorSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="description" minOccurs="0" />
            <xs:element ref="weavingfactors" minOccurs="0" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="project_id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="optional" 
            default="" />
      </xs:complexType>
   </xs:element>

   <xs:element name="weavingfactors">
      <xs:complexType mixed="true">   <!-- text in normalized units -->
         <xs:attribute name="vehicle_type_id" type="xs:long" 
            use="required" />
         <xs:attribute name="link_in" type="xs:long" use="required" />
         <xs:attribute name="link_out" type="xs:long" use="required" />
      </xs:complexType>
   </xs:element>

<!-- SplitRatioSet ...................................................... -->

   <xs:element name="SplitRatioSet">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="description" minOccurs="0" />
            <xs:element ref="splitRatioProfile" minOccurs="0" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="id" type="xs:long" use="required"/>
         <xs:attribute name="project_id" type="xs:long" use="required" />
          <xs:attribute name="network_id" type="xs:long" use="required" />
         <xs:attribute name="name" type="xs:string" use="optional" 
            default="" />
         <xs:attribute name="mod_stamp" type="xs:string" use="optional" 
            default="0" />
         <xs:attribute name="lockedForEdit" type="xs:boolean" use="optional" 
            default="false" />
         <xs:attribute name="lockedForHistory" type="xs:boolean" 
            use="optional" default="false" />
      </xs:complexType>
   </xs:element>

   <xs:element name="splitRatioProfile">
      <xs:complexType>
         <xs:sequence>
            <xs:element ref="splitratio" minOccurs="0" 
               maxOccurs="unbounded" />
         </xs:sequence>
         <xs:attribute name="crudFlag" type="xs:string" use="optional"/>
         <xs:attribute name="id" type="xs:long" use="required" />
         <xs:attribute name="node_id" type="xs:long" use="required" />
